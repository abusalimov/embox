package embox.example.mem

module boundary_markers {
	source "boundary_markers.c"

	depends embox.mem.pagealloc.page_api
	depends embox.framework.example.example
}

//$_BRIEF-objalloc = demonstrates using of API for allocation fixed size object
module objalloc {
	source "objalloc.c"

	depends embox.compat.libc.all
	depends embox.mem.objalloc.objalloc
	depends embox.framework.example.example
}

module page {
	source "page.c"

	depends embox.mem.pagealloc.page_api
	depends embox.framework.example.example
}

module pool {
	source "pool.c"

	depends embox.mem.misc.pool
	depends embox.framework.example.example
}

module simple_alloc {
	option number pool_size=65535

	source "simple_alloc.c"

	depends embox.compat.libc.all
	depends embox.framework.example.example
}
