package embox.drivers.block_dev.flash

module flash_lib {
	@IncludeExport(path="drivers/block_dev/flash")
	source "flash.h"
	@IncludeExport(path="drivers/block_dev/flash")
	source "flash_dev.h"
}

module flash {
	source "flash.c"

	option number dev_quantity=4

	depends flash_lib

	depends embox.fs.node
	depends embox.mem.pagealloc.page_api
	depends embox.mem.misc.pool
	depends embox.drivers.block_dev.block
	depends embox.mem.phymem
	depends embox.util.indexator
}
