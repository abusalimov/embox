package embox.net.l3.ipv4

module ipv4 {
	option number ip_fragmented_support = 1
	option number max_uncomplete_cnt = 16

	source "ip_fragment.c"

	depends embox.net.skbuff
	depends embox.mem.objalloc.objalloc
	depends embox.kernel.time.timer.sys_timer

	source "ip_input.c"
	depends embox.net.skbuff
	depends embox.net.socket.raw_sock
	/* depends from ip_fragment.c */
	depends embox.net.netfilter.netfilter

	source "ip_options.c"
	depends embox.net.l3.icmpv4

	source "ip_output.c"

	source "ip_header.c"

	depends embox.net.l0.net_tx
	depends embox.net.skbuff
	depends embox.net.l3.route
	depends embox.net.packet
	depends embox.net.lib.ipv4
}
