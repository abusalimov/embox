package embox.compat.posix.proc

static module exec_exchanged extends embox.compat.posix.proc.exec_lib {
	source "exec_exchanged.c"

	depends embox.kernel.task.resource.argv.argv
	depends embox.compat.posix.proc.exec_lib
}

static module vfork_exchanged extends embox.compat.posix.proc.vfork {
	source "vfork_exchanged.c"
	source "vfork_res_exchanged.h"

	depends embox.compat.posix.proc.exit
	depends embox.kernel.task.resource.vfork.vfork
	depends embox.compat.posix.proc.vfork_entry
	depends embox.arch.vfork_entry
	depends embox.util.LibUtil
}
