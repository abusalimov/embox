package embox.compat.posix

module LibPosix {
	depends embox.compat.posix.fs.all
	depends embox.compat.posix.idx.All
	depends embox.compat.posix.net.All
	depends embox.compat.posix.proc.All
	depends embox.compat.posix.pwd_grp.passwd
	depends embox.compat.posix.utmp.utmp_api
	depends embox.compat.posix.util.All
	depends embox.compat.posix.pthread.pthreads
	depends sched
	depends termios
	depends embox.kernel.task.resource.errno
}

static module pty {
	source "pty.c"
}

static module sched {
	source "sched_posix.c"
}

static module termios {
	source "termios.c"
	depends embox.drivers.tty.tty
}

static module semaphore {
	source "semaphore.c"
}
