package embox.fs.driver.nfs

module nfs {
	@IncludeExport(path="fs")
	source "nfs.h"
	@IncludeExport(path="fs")
	source "xdr_nfs.h"

	source "nfs.c"
	source "xdr_nfs.c"
	option number inode_quantity=264
	option number nfs_descriptor_quantity=4

	depends embox.net.lib.rpc.rpc
	depends embox.fs.node, embox.fs.driver.repo
	depends embox.fs.driver.devfs.devfs
	depends embox.mem.pagealloc.page_api
	depends embox.mem.misc.pool
}

