package embox.fs.driver.iso9660

module cdfs {
	@IncludeExport(path="fs")
	source "iso9660.h"
	@IncludeExport(path="fs")
	source "iso9660_format.h"

	source "cdfs.c"
	option number inode_quantity=2048
	option number cdfs_descriptor_quantity=4

	depends embox.fs.node, embox.fs.driver.repo
	depends embox.drivers.block_dev.block
	depends embox.mem.pagealloc.page_api
	depends embox.mem.misc.pool
}

